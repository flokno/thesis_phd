@article{martin1976,
  title = {Phonon Scattering in {{RbCa}}\$\{\textbackslash mathrm\{\vphantom{\}\}}{{F}}\vphantom\{\}\vphantom\{\}\_\{3\}\$ and {{KMn}}\$\{\textbackslash mathrm\{\vphantom{\}\}}{{F}}\vphantom\{\}\vphantom\{\}\_\{3\}\$},
  author = {Martin, J. J. and Dixon, G. S. and Velasco, P. P.},
  year = {1976},
  month = sep,
  volume = {14},
  pages = {2609--2612},
  doi = {10.1103/PhysRevB.14.2609},
  abstract = {Low-temperature thermal-conductivity measurements have been made on the perovskite compounds RbCaF3 and KMnF3. Both RbCaF3 and KMnF3 have anomalous thermal conductivities with a strong minimum near their lower-temperature structure change, 45 and 92 K, respectively; and the conductivities of both materials approach a normal lattice conductivity at the high temperatures. The 198- and 187-K cubic-to-tetragonal transitions appear to have only a small effect on the thermal conductivity. The thermal conductivity of both materials appears to be limited by phonon scattering associated with soft modes at the structural phase transitions.},
  journal = {Physical Review B},
  number = {6}
}

@incollection{moore1985,
  title = {The {{Thermal Conductivities}} of {{SrCl2}} and {{SrF2}} from 85 to 400 {{K}}},
  booktitle = {Thermal {{Conductivity}} 18},
  author = {Moore, J. P. and Weaver, F. J. and Graves, R. S. and McElroy, D. L.},
  editor = {Ashworth, T. and Smith, David R.},
  year = {1985},
  pages = {115--124},
  publisher = {{Springer US}},
  address = {{Boston, MA}},
  doi = {10.1007/978-1-4684-4916-7_12},
  abstract = {Measurements of thermal conductivity, {$\lambda$}, for the alkaline earth compounds SrCl2 and SrF2 indicate that the {$\lambda$} of SrC12 is less than that of SrF2 by a factor of 4 to 5 over the temperature range from 85 to 400 K. This difference can be related to the differences in other parameters of the two compounds. At the lower end of the temperature range, the thermal resistivities, {$\lambda-$}1, of both materials are linear; but large positive deviations occur at the higher end of the range. Calculated values using an equation for acoustic phonon conduction agree with the experimental values to within 30\% over the entire measurement range. The positive deviations from linearity appear to be due to the effect of lattice dilation on the Debye temperatures and Gr\"uneisen constants.},
  isbn = {978-1-4684-4916-7},
  keywords = {Debye Temperature,Large Positive Deviation,Lattice Dilation,Strontium Chloride,Thermal Resistivity},
  language = {English}
}

