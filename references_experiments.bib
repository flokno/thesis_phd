@article{li2003,
  title = {Fabrication of Transparent {{Sc}} {$_2$} {{O}} {$_3$} Ceramics with Powders Thermally Pyrolyzed from Sulfate},
  author = {Li, Ji-Guang and Ikegami, Takayasu and Mori, Toshiyuki},
  year = {2003},
  month = aug,
  volume = {18},
  pages = {1816--1822},
  issn = {0884-2914, 2044-5326},
  doi = {10.1557/JMR.2003.0252},
  abstract = {Scandia (Sc 2 O 3 ) is a ceramic material that shows interesting thermal and optical properties, but is difficult to grow as single crystals. As an alternative, in this work we fabricated polycrystalline Sc 2 O 3 transparent ceramics via vacuum sintering, using powders thermally pyrolyzed at 1200 \textdegree C from a scandium sulfate salt, Sc 2 (SO 4 ) 3 {$\cdot$} 7.8H 2 O, that we prepared. Characterization of the powders was achieved by differential thermal analysis/thermogravimetry, x-ray diffractometry, Brunauer\textendash Emmett\textendash Teller analysis, and field-emission scanning electron microscopy. Sintering behaviors of the Sc 2 O 3 powders were studied in air via dilatometry. The sulfate salt transforms to oxide at temperatures {$\geq$}1000 \textdegree C, and the best pyrolysis temperature for transparent ceramics fabrication is 1200 \textdegree C, at which the resultant Sc 2 O 3 powder is good in dispersion, ultrafine in particle size ({$\sim$}80 nm), and almost free from residual sulfur. Transparent ceramics were fabricated from this powder via vacuum sintering at 1625 \textdegree C or above. The ceramics sintered at 1700 \textdegree C for 4 h exhibit an in-line transmittance of approximately 56\textendash 58\% in the visible light region at a sample thickness of 1.0 mm.},
  journal = {Journal of Materials Research},
  language = {English},
  number = {8}
}

