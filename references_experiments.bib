@article{suemune1964,
  title = {Thermal {{Conductivity}} of {{KMnF3}}, {{KCoF3}}, {{KNiF3}}, and {{KZnF3 Single Crystals}}},
  author = {Suemune, Yasutaka and Ikawa, Hy{\=o}},
  year = {1964},
  month = sep,
  volume = {19},
  pages = {1686--1690},
  issn = {0031-9015},
  doi = {10.1143/JPSJ.19.1686},
  abstract = {The thermal conductivity of single crystals of KMnF 3 , KCoF 3 , KNiF 3 , and KZnF 3 has been measured in the temperature range of 15\textdegree{} to 300\textdegree K by the absolute longitudinal heat-flow method. These compounds have the same crystal structure of the perovskite type and are antiferromagnetic except for diamagnetic KZnF 3 . The anomalous decreases are shown in the temperature dependence of the thermal conductivity for the magnetic crystals of KMnF 3 and KCoF 3 at their N\'eel temperatures of 88\textdegree{} and 114\textdegree K respectively, but KNiF 3 has no anomaly at its N\'eel temperature of 275\textdegree K. These anomalies are seemed to be due to the phonon-magnon scattering similarily to the cases of some magnetic substances. The diamagnetic KZnF 3 shows the normal behaviour of conduction, though the slope of the thermal conductivity versus temperature is somewhat lower owing to the impurity scattering of phonons than that being expected from Umklapp processes.},
  journal = {Journal of the Physical Society of Japan},
  number = {9}
}

